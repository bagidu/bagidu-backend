{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage-e2e",
  "collectCoverageFrom": [
    "**/src/**/*.{js,ts}",
    "!**/src/**/*.spec.{js,ts}",
    "!**/node_modules/**",
    "!**/dist/**"
  ],
  "coverageReporters": [
    "text",
    "html",
    "lcov",
    "clover"
  ]
}